public with sharing class Occurrence {
    public static void Count(String s)
    {
        String tolower = s.toLowerCase();
        String dws = tolower.deleteWhitespace();
        String [] str = dws.split('');
        Set<String> set1 =new Set<String>(str);
        
        for(String character:set1)
        {
            system.debug(character+'\t'+dws.countMatches(character));
        }
        
    }
    
}